<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\SurveySolutionsClient.xml</DocumentationFile>
        <RepositoryUrl>https://github.com/SlyNet/SurveySolutionsClient.git</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <AssemblyVersion>1.0.0.0</AssemblyVersion>
        <Version>2.0.1.0</Version>
        <Authors>Andrii Kozhyn</Authors>
        <PackageProjectUrl>https://docs.mysurvey.solutions/headquarters/api/survey-solutions-api/</PackageProjectUrl>
        <PackageReleaseNotes>Fixed issue with getting assignment details and ability to assign workspace for existing user</PackageReleaseNotes>
        <Description>Client for survey solutions application</Description>
        <NoWarn>1701;1702;1591;8618</NoWarn>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <DefineConstants>GRAPHQL_GENERATOR_DISABLE_NEWTONSOFT_JSON</DefineConstants>
    </PropertyGroup>
    <ItemGroup>
      <PackageReference Include="System.Text.Json" Version="5.0.0" />
    </ItemGroup>
</Project>
